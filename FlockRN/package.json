{
  "name": "flock-baby-shower",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "dev": "expo start --go",
    "dev:android": "expo start --go --android",
    "dev:ios": "expo start --go --ios",
    "dev:web": "expo start --go --web",
    "start": "expo start --dev-client",
    "test": "jest --watchAll",
    "test:ci": "jest --ci",
    "lint": "eslint .",
    "lint:ci": "expo lint",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "generate:types": "ts-node schema/generateTypes.ts",
    "fix:sync": "ts-node --project tsconfig.scripts.json scripts/fixSyncActions.ts",
    "clean": "rm -rf node_modules && rm -rf ios && rm -rf android",
    "clean:ios": "rm -rf ios",
    "clean:android": "rm -rf android",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native|@firebase|firebase|expo(nent)?|@expo(nent)?|@expo-google-fonts|react-navigation|@react-navigation|unimodules|sentry-expo|native-base|react-native-svg|react-native-reanimated|react-native-gesture-handler)"
    ],
    "moduleNameMapper": {
      "^firebase/(.*)$": "<rootDir>/__mocks__/firebase/$1",
      "^@/(.*)$": "<rootDir>/$1"
    },
    "testEnvironment": "jsdom",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,ts,tsx}"
    ]
  },
  "dependencies": {
    "@expo/metro-config": "~0.20.14",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-picker/picker": "2.11.1",
    "@react-native-vector-icons/common": "^12.0.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/native": "^7.1.6",
    "@sentry/react-native": "7.0.0-beta.2",
    "@shopify/flash-list": "1.7.6",
    "@tanstack/react-query": "^5.80.7",
    "dotenv": "^16.5.0",
    "expo": "53.0.19",
    "expo-audio": "~0.4.8",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-crypto": "~14.1.5",
    "expo-dev-client": "~5.2.4",
    "expo-file-system": "^18.1.11",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.3",
    "expo-speech-recognition": "^2.0.0",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-updates": "~0.28.17",
    "expo-web-browser": "~14.2.0",
    "firebase": "^11.7.3",
    "later": "^1.2.0",
    "lodash": "^4.17.21",
    "openai": "^4.100.0",
    "or": "^0.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-calendars": "^1.1312.0",
    "react-native-collapsible": "^1.6.2",
    "react-native-haptic-feedback": "^2.3.3",
    "react-native-modal": "^14.0.0-rc.1",
    "react-native-reanimated": "^3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "^4.11.1",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.15.19",
    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "^19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "jest": "~29.7.0",
    "jest-expo": "~53.0.9",
    "prettier": "^3.5.3",
    "react-test-renderer": "19.0.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3"
  },
  "private": true
}
